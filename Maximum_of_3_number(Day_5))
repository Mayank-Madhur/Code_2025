public class Maximum_of_3_number {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 17;
		int b = 9;
		int c = 5;
		if (a >= b && a >= c) {
			System.out.println(a);
		} else if (b >= a && b >= c) {
			System.out.println(b);
		} else {
			System.out.println(c);
		}
	}

}
